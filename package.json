{
  "name": "nano",
  "version": "1.0.0",
  "description": "bot",
  "main": "dist/index.js",
  "types": "dist",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false
  },
  "scripts": {
    "prettier": "prettier src --write",
    "build": "git rm -r --ignore-unmatch dist && npm run prettier && tsc",
