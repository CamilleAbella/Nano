{
  "name": "@ghom/nano-bot",
  "author": "ghom",
  "license": "ISC",
  "version": "v1.10.17-beta",
  "description": "Discord bot core in TypeScript.",
  "main": "dist/index.js",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false,
    "useTabs": false
  },
  "scripts": {
    "prettier": "prettier src scripts package.json assets/schemas/nano.config.schema.json assets/schemas/nano.module.schema.json readme.md tsconfig.json --write",
    "build": "git rm -r --ignore-unmatch -f ./dist && npm run prettier && tsc",
    "precommit": "node scripts/pre-commit && npm run prettier && git add *",
    "commit": "git commit -m \"patch core to v1.10.17-beta\"",
    "push": "npm run commit && git push origin master"
  },
  "keywords": [
    "discord.js",
    "bot",
    "typescript",
    "handler",
    "command-handler"
  ],
  "engines": {
    "node": "v14.3.0",
    "npm": "6.14.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CamilleAbella/Nano.git"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/ws": "^7.2.6",
    "prettier": "^2.0.5",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "better-sqlite3": "7.1.0",
    "chalk": "4.1.0",
    "discord.js": "12.3.1",
    "dotenv": "8.2.0",
    "enmap": "^5.7.1"
  }
}
