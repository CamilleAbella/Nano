    "push": "git add * && git commit -m 'continue' && git push origin master",
    "build.push": "npm run build && npm run push"
  },
  "keywords": [],
  "author": "ghom",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/ws": "^7.2.6",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "better-sqlite-pool": "^0.3.0",
    "better-sqlite3": "^7.1.0",
    "discord.js": "^12.3.1",
    "enmap": "^5.6.1"
  }
}
